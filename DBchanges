create table address (id bigint not null auto_increment, address1 varchar(255), address2 varchar(255), city varchar(255), postalcode varchar(255), state varchar(255), primary key (id)) engine=InnoDB
create table guest (ghid bigint not null auto_increment, birth_date date, create_date date, email varchar(255), first_name varchar(255), last_name varchar(255), last_stay date, phno varchar(255), purge_date date, address_id bigint, primary key (ghid)) engine=InnoDB
alter table guest add constraint UK_fqowrdlybnwhaop4gkiwtybsd unique (address_id)